<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd"
       default-autowire-candidates="*er" default-autowire="byName">
    <bean id="teacher" class="spring.model.depend.Teacher">
        <constructor-arg name="name" value="zhangsan"/>
    </bean>
    <bean id="Ateacher" class="spring.model.depend.Teacher">
        <constructor-arg name="name" value="lisi"/>
    </bean>
    <bean class="spring.model.depend.Student"/>
</beans>